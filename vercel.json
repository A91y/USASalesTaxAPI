{
    "devCommand": "gunicorn USASalesTaxAPI.wsgi:application --log-file -",
    "builds": [
      {
        "src": "USASalesTaxAPI/wsgi.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "USASalesTaxAPI/wsgi.py"
      }
    ],
    "installCommand": "pip install -r requirements.txt"
}